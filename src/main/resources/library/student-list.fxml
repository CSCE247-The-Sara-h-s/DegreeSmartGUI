<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import java.net.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" <VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="degreesmart.project.StudentListController">
>
    <children>
      <Label text="Student List" />
   </children>
    <TableView fx:id="tableView">

        <columns>
            <TableColumn prefWidth="212.3" text="Name" fx:id="nameCol"  >
                <cellValueFactory>
                    <PropertyValueFactory property="name" />
                </cellValueFactory>
            </TableColumn>

            <TableColumn prefWidth="212.3" text="Year" fx:id="yearCol" >
                <cellValueFactory>
                    <PropertyValueFactory property="year" />
                </cellValueFactory>
            </TableColumn>


            <TableColumn prefWidth="212.3" text="Major" fx:id="majorCol" >
                <cellValueFactory>
                    <PropertyValueFactory property="major" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
       <stylesheets>
      <URL value = "@table.css"/>
   </stylesheets>

</VBox>